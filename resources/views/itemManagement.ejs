                        </a>
                    </th>
                    
                </tr>
                <tr>
                    <th class="px-4 py-2 text-left">ID</th>
                    <th class="px-4 py-2 text-left">Name</th>
                    <th class="px-4 py-2 text-left">Image</th>
                    <th class="px-4 py-2 text-left">Length</th>
                    <th class="px-4 py-2 text-left">Width</th>
                    <th class="px-4 py-2 text-left">Height</th>
                    <th class="px-4 py-2 text-left">Material Used</th>
                    <th class="px-4 py-2 text-left">Colors</th>
                    <th class="px-4 py-2 text-left">Fabrics</th>
                    <!-- <th class="px-4 py-2 text-left">Action</th> -->
                </tr>
            </thead>
            <tbody>
                <% trackings.forEach(function(tracking) { %>
                <tr>
                    <td class="border px-4 py-2">
                        <%= tracking._id %>
                    </td>
                    <td class="border px-4 py-2">
                        <%= tracking.name %>
                    </td>
                    <td class="border px-4 py-2">

                    </td>
                    <td class="border px-4 py-2">
                        <%= tracking.length %>
                    </td>
                    <td class="border px-4 py-2">
                        <%= tracking.width %>
                    </td>
                    <td class="border px-4 py-2">
                        <%= tracking.height %>
                    </td>
                    <td class="border px-4 py-2">
                        <%= tracking.materialusage %>
                    </td>
                    <td class="border px-4 py-2">
                        <%= tracking.colors %>
                    </td>
                    <td class="border px-4 py-2">
                        <%= tracking.fabrics %>
                    </td>
